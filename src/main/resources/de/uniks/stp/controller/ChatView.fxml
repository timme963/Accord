<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox id="root" fx:id="topVbox" prefHeight="2110.0" prefWidth="3469.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <StackPane id="stack" alignment="BOTTOM_RIGHT" prefHeight="2074.0" prefWidth="200.0">
            <children>
                <VBox id="emojiBox" alignment="BOTTOM_RIGHT" maxHeight="446.0" maxWidth="379.0" minHeight="446.0" minWidth="379.0" prefHeight="446.0" prefWidth="379.0" style="-fx-background-color: transparent;" />
                <ScrollPane id="messageScrollPane" hbarPolicy="NEVER" vbarPolicy="NEVER">
                    <content>
                        <VBox id="messageVBox" alignment="TOP_CENTER" />
                    </content>
                </ScrollPane>
            </children>
        </StackPane>
        <HBox id="container" alignment="CENTER_LEFT" prefHeight="1.0" prefWidth="3469.0" />
        <HBox id="messageBox" alignment="CENTER" maxHeight="35.0" prefHeight="35.0" prefWidth="400.0" spacing="20.0">
            <children>
                <HBox prefHeight="35.0" prefWidth="4.0" />
                <TextField fx:id="messageTextField" style="-fx-background-radius: 6; -fx-border-radius: 6;" />
                <Button id="emojiButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="30.0" style="-fx-background-radius: 20;">
                    <graphic>
                        <ImageView id="imageView" fitHeight="19.0" fitWidth="29.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../icons/grinning-face.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button id="sendButton" fx:id="sendButton" mnemonicParsing="false" text="%button.send" textFill="WHITE" />
                <HBox prefHeight="35.0" prefWidth="4.0" />
            </children>
        </HBox>
    </children>
</VBox>
