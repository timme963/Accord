<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" id="openSnakeBox" alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.1">
    <children>
        <HBox id="titleBarBox"/>
        <VBox alignment="CENTER" prefHeight="89.0" prefWidth="453.0">
            <children>
                <Label id="label_congrats" prefHeight="36.0" prefWidth="185.0" text="Congratulation!">
                    <font>
                        <Font size="25.0"/>
                    </font>
                </Label>
                <Label id="label_you-found" prefHeight="36.0" prefWidth="291.0" text="You found the easter egg!">
                    <font>
                        <Font size="25.0"/>
                    </font>
                </Label>
            </children>
            <VBox.margin>
                <Insets top="10.0"/>
            </VBox.margin>
        </VBox>
        <HBox alignment="CENTER" prefHeight="50.0" prefWidth="453.0">
            <children>
                <Button id="button_start" mnemonicParsing="false" prefHeight="50.0" prefWidth="155.0" text="Start">
                    <font>
                        <Font size="23.0"/>
                    </font>
                </Button>
            </children>
            <VBox.margin>
                <Insets bottom="15.0"/>
            </VBox.margin>
        </HBox>
        <HBox alignment="CENTER" prefHeight="50.0" prefWidth="453.0">
            <children>
                <Button id="button_exit" mnemonicParsing="false" prefHeight="50.0" prefWidth="155.0" text="Exit">
                    <font>
                        <Font size="23.0"/>
                    </font>
                </Button>
            </children>
            <VBox.margin>
                <Insets bottom="15.0"/>
            </VBox.margin>
        </HBox>
    </children>
</VBox>
