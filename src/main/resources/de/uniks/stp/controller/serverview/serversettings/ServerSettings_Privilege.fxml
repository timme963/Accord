<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" id="rootPrivilege" prefHeight="400.0" prefWidth="400.0"
      xmlns="http://javafx.com/javafx/11">
    <children>
        <HBox id="select" alignment="BOTTOM_CENTER" prefHeight="145.0" prefWidth="400.0" spacing="10.0">
            <children>
                <VBox alignment="CENTER" prefHeight="113.0" prefWidth="90.0" spacing="40.0">
                    <children>
                        <Label style="-fx-font-size: 16;" text="%label.category" textFill="WHITE"/>
                        <Label style="-fx-font-size: 16;" text="%label.channel" textFill="WHITE"/>
                    </children>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                </VBox>
                <VBox alignment="CENTER" prefHeight="123.0" prefWidth="121.0" spacing="40.0">
                    <children>
                        <ComboBox id="Category" prefWidth="137.0" promptText="%comboBox.selectCategory"/>
                        <ComboBox id="Channels" prefWidth="150.0" promptText="%comboBox.selectChannel"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER_LEFT" prefHeight="102.0" prefWidth="142.0" spacing="20.0">
                    <children>
                        <Label style="-fx-font-size: 16;" text="%label.turnPrivilege" textFill="WHITE">
                            <VBox.margin>
                                <Insets top="-5.0"/>
                            </VBox.margin>
                        </Label>
                        <VBox>
                            <children>
                                <HBox alignment="CENTER_LEFT" prefWidth="100.0" spacing="20.0">
                                    <children>
                                        <Label text="%label.on" textFill="WHITE"/>
                                        <Label text="%label.off" textFill="WHITE"/>
                                    </children>
                                    <opaqueInsets>
                                        <Insets/>
                                    </opaqueInsets>
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </HBox>
                                <HBox alignment="CENTER" spacing="20.0">
                                    <children>
                                        <RadioButton id="Privilege_On_Button" mnemonicParsing="false" textFill="WHITE"/>
                                        <RadioButton id="Privilege_Off_Button" mnemonicParsing="false"/>
                                        <Button id="Change_Privilege" mnemonicParsing="false" prefHeight="30.0"
                                                prefWidth="70.0" text="%button.change" textFill="WHITE"/>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <HBox.margin>
                        <Insets top="10.0"/>
                    </HBox.margin>
                </VBox>
            </children>
        </HBox>
        <HBox id="Privilege_On" prefHeight="300.0" prefWidth="400.0"/>
    </children>
</VBox>
