<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1">
    <children>
        <HBox id="titleBarBox"/>
        <Pane id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
              prefHeight="400.0" prefWidth="500.0">
            <children>
                <VBox id="settingsItems" alignment="TOP_CENTER" prefHeight="400.0" prefWidth="100.0" spacing="5.0">
                    <children>
                        <Button id="overviewBtn" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0"
                                text="%button.overview" textFill="WHITE"/>
                        <Button id="channelBtn" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0"
                                text="%button.channel" textFill="WHITE"/>
                        <Button id="categoryBtn" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0"
                                text="%button.category" textFill="WHITE"/>
                        <Button id="privilegeBtn" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0"
                                text="%button.privilege" textFill="WHITE"/>
                    </children>
                    <padding>
                        <Insets top="10.0"/>
                    </padding>
                </VBox>
                <VBox id="serverSettingsContainer" layoutX="100.0" prefHeight="400.0" prefWidth="400.0"/>
            </children>
        </Pane>
    </children>
</VBox>
