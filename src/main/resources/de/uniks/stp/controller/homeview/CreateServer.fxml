<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" id="createServerBox" alignment="CENTER" prefWidth="480.0" spacing="15.0"
      xmlns="http://javafx.com/javafx/11">
    <children>
        <Label id="create_errorLabel" textFill="#fc0000" VBox.vgrow="ALWAYS">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <TextField id="serverName" promptText="%textField.server_name">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0"/>
            </VBox.margin>
        </TextField>
        <Button id="createServer" mnemonicParsing="false" prefHeight="30.0" prefWidth="70.0" text="Create"/>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
